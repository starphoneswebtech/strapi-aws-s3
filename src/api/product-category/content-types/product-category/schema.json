{
  "kind": "collectionType",
  "collectionName": "product_categories",
  "info": {
    "singularName": "product-category",
    "pluralName": "product-categories",
    "displayName": "Product Category",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "metadata": {
      "type": "component",
      "repeatable": false,
      "component": "metadata.metadata"
    },
    "banner": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "medusaIdentifier": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "faqs": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::faq.faq"
    },
    "rank": {
      "type": "integer"
    },
    "featureImage": {
      "allowedTypes": [
        "files",
        "images"
      ],
      "type": "media",
      "multiple": false
    }
  }
}
