{
  "kind": "collectionType",
  "collectionName": "states",
  "info": {
    "singularName": "state",
    "pluralName": "states",
    "displayName": "State",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "code": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "text"
    },
    "handle": {
      "type": "string",
      "required": true,
      "regex": "^[A-Za-z-]+$"
    },
    "featuredImage": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "store_locations": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::store-location.store-location"
    },
    "rank": {
      "type": "integer"
    }
  }
}
